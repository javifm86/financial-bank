{
  "name": "financial-bank",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel mock-api serve",
    "mock-api": "json-server --watch ./server-api/db.json --port 3000 --delay 400",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "concat-api-db": "json-concat ./server-api/src/ ./server-api/db.json",
    "postconcat-api-db": "prettier --write ./server-api/db.json"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "highcharts": "^8.2.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "json-concat": "0.0.1",
    "json-server": "^0.16.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  }
}
